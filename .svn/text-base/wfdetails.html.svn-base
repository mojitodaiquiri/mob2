<!DOCTYPE HTML>
<html>
	<head>
		<title>iAgree</title>
		<link rel="apple-touch-icon" href="touch-icon-iphone.png" />
		<link rel="apple-touch-icon" sizes="72x72" href="touch-icon-ipad.png" />
		<link rel="apple-touch-icon" sizes="114x114" href="touch-icon-iphone4.png" />
		<meta name="viewport" content="width=device-width,initial-scale=1"/>
		<!--
		<meta name="apple-mobile-web-app-capable" content="yes" />
		-->
		<link rel="stylesheet" href="css/jquery.mobile-1.1.0-rc.1.css" />
		<link rel="stylesheet" href="css/jquery.mobile.simpledialog.min.css" />
		<link rel="stylesheet" href="css/styles.css" />
		<!--
		-->
	</head>
	<body>
		<div id="detailsPage" data-role="page" data-add-back-btn="true">
			<div data-role="header" data-position="fixed">
				<h1></h1>
			</div>
			<div data-role="content">
				<img id="wfPic"/>
				<div id="wfHeader">
					<h3 id="subject"></h3>
					<p id="description"></p>
					<p id="property1"></p>
					<p id="property2"></p>
				</div>
				<br />
				<ul id="propertyList" data-role="listview" data-inset="true"></ul>
				<div id="attachments" data-role="collapsible" style="display: none">
					<h3>Allegati <img src="pics/attachment.png" style="float:right;"/></h3>
					<p>
						<ul id="attachmentsList" data-role="listview" data-inset="true"></ul>
					</p>
				</div>
				<div id="contacts" data-role="collapsible" style="display: none">
					<h3>Contatti <img src="pics/contacts.png" style="float:right;"/></h3>
					<p>
						<ul id="contactsList" data-role="listview" data-inset="true"></ul>
					</p>
				</div>
				<fieldset class="ui-grid-a">
					<div id="divapp" class="ui-block-a" style="display: none">
						<a href="wfpopupConfirm.html" data-theme="g"  data-role="button"  data-rel="dialog" data-transition="pop">Approvo</a>
						<!--
						<button id="btnapp" type="submit" >
						Approvo
						</button>
						-->
					</div>
					<div id="divrej" class="ui-block-b" style="display: none">
						<a href="wfpopupReject.html" data-theme="f" data-role="button"  data-rel="dialog" data-transition="pop">Rifiuto</a>
						<!--
						<button id="btnrej" type="submit" data-theme="f">
						Rifiuto
						</button>
						-->
					</div>
				</fieldset>
			</div>
		</div>
		<script src="js/jquery.js"></script>
		<script src="js/personalizzazioni.js"></script>
		<script src="js/jquery.mobile-1.1.0-rc.1.js"></script>
		<script src="js/wflist.js"></script>
		<script src="js/wfdetails.js"></script>
	</body>
</html>
